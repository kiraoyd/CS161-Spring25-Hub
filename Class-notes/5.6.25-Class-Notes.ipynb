   "id": "33550806-8f77-47f0-b106-4148a9948678",
   "metadata": {},
   "outputs": [],
   "source": [
    "def sum_sequence(n):\n",
    "    count = 1\n",
    "    my_sum = 0\n",
    "    while count <= n:\n",
    "        #do something\n",
    "        my_sum = my_sum + count\n",
    "        count = count + 1\n",
    "\n",
    "    print(f\"The sum of 1 to {n} is {my_sum}\")\n",
    "\n",
    "def main():\n",
    "    n = 100000000\n",
    "    sum_sequence(n)\n",
    "\n",
    "main()"
   ]
